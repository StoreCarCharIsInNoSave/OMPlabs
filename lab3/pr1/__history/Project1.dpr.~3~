program Project1;
(*


. Вычислить значение функции f на отрезке [a,b] с заданным шагом h.
 Результаты представить в виде таблицы. Вычисление суммы,
  произведения, максимального или минимального элемента и функции f
   оформить в виде отдельных функций пользователя c передачей параметров.
f=x*x+tg(x) при x<2
f = (x-5)/(1+x*x) при 2<=x<=10
f = (1+x)/(x*x*x)* max(1/x,sqrt(x+5)) при x>10
*)
{$APPTYPE CONSOLE}
{$R *.res}

uses
  System.SysUtils , math;

function Sum(a: double; b: double): double;
begin
  result := a + b;
end;

function Product(a: double; b: double): double;
begin
  result := a * b;
end;

 function Divide(a: double; b: double): double;
begin
  result := a * b;
end;

 function Difference(a: double; b: double): double;
begin
  result := a * b;
end;

function Max(a: double; b: double): double;
begin
  if a > b then
    result := a
  else
    result := b;
end;

function Min(a: double; b: double): double;
begin
  if a < b then
    result := a
  else
    result := b;
end;

var x,b,h,f:double;
begin
 writeln('Введите нижнюю границу, верхнюю границу, шаг');
 readln(x,b,h);
 while x<=b do begin
  if x<2 then f:=Sum(Product(x,x),tan(x))
  else if x<=10 then f:=Divide(Difference(x,5),Sum(1,Product(x,x)))
  else
  begin
      f:=Product(Divide(Sum(1,x),Product(x,Product(x,x))),max(divide(1,x),sqrt(sum(x,5))));
  end;
  Writeln('X= ',x:3:3);
  writeln('f = ',f:3:3);
 x:=x+h;

 end;
 readln;
end.
