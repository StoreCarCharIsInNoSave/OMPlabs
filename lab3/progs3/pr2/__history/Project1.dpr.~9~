program Project1;
(*
  Орловский ПМ-12 Вариант 15.
  5.	Даны два целочисленных вектора А(n), B(n). Подсчитать количество таких i,
  для которых : а) А [i] < В [i]; б) А [i]=В [i]; в) А [i]>В [i].
*)
{$APPTYPE CONSOLE}
{$R *.res}

uses
  System.SysUtils;

procedure EqualArrays(var vector1: array of integer;
  var vector2: array of integer; const sizeVector1: integer;
  const sizeVector2: integer);
var
  less, more, equal: integer;
begin
  for var i := 0 to sizeVector1 - 1 do
    for var j := 0 to sizeVector2 - 1 do
    begin
      if vector1[i] < vector2[j] then
        inc(less)
      else if vector1[i] = vector2[j] then
        inc(equal)
      else
        inc(more);

    end;
  writeln('Количество i, где A[i]<B[i] = ', less);
  writeln('Количество i, где A[i]=B[i] = ', equal);
  writeln('Количество i, где A[i]<B[i] = ', more);
end;


var A,B:array of integer;
var sizea,sizeb:integer;
begin
writeln('Введите размер массива A');
readln(sizea);
writeln('Введите размер массива B');
readln(sizeb);



end.
