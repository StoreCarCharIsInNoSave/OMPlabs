program Project1;

{$APPTYPE CONSOLE}
{$R *.res}

uses
  System.SysUtils;

var
  x, k, h, f: double;

var
  i: integer;
  state: string;

begin
  writeln('Введите x,k,h');

  readln(x, k, h);
  writeln('╔════════╦══════════╗');
  writeln('║    x   ║   f(x)   ║');
  writeln('╠════════╬══════════╣');
  while x < k do
  begin
    if x < 1 then
    begin
      state := 'x<1';
      f := (x + 1) / (1 - x * x);
    end
    else if (1 <= x) and (x <= 8) then

    begin
      state := '1<=x<=8';
      f := sqrt(abs(x * x * x - 1));
      for i := 1 to 5 do
        f := f + (i - 2) * (i - 2);
    end
    else
    begin
      state := 'x>8';
      f := (x + 5) / 12 + x + x * x;
    end;

    writeln('║',x:2:2,'   ║ ',f:2:2,'   ║');

    x := x + h;
  end;
    writeln('╚═══════════════════╝');
  readln;

end.
