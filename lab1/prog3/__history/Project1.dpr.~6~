program Project1;

{$APPTYPE CONSOLE}
{$R *.res}

uses
  System.SysUtils;

var
  x, k, h, f: double;

var
  flag: byte;

var
  i: integer;
  state: string;

 function SizeOrganizer(normalize:integer; number:double):integer;
 begin
     result:=length(floattostr(number))-normalize;
 end;
  
begin
  writeln('Введите x,k,h');
       writeln(SizeOrganizer(3,-0.23)) ;
  readln(x, k, h);
  writeln('╔════════╦══════════╗');
  writeln('║    x   ║   f(x)   ║');
  writeln('╠════════╬══════════╣');
  while x < k do
   

  begin
   flag := 1;
    if x < 1 then
    begin
    
      if (1 - x * x) = 0 then
        flag := 0
      else
        f := (x + 1) / (1 - x * x);
    end
    else if (1 <= x) and (x <= 8) then
    begin
      f := sqrt(abs(x * x * x - 1));
      for i := 1 to 5 do
        f := f + (i - 2) * (i - 2);
    end
    else
      f := (x + 5) / 12 + x + x * x;
      case flag of
      1:   begin
      
      end;
      writeln('║', x:2:2, '   ║ ', f:2:2, '   ║');
      0:writeln('║', x:2:2, '   ║ except   ║');
      end;

    

    x := x + h;
  end;
  writeln('╚═══════════════════╝');
  readln;

end.
